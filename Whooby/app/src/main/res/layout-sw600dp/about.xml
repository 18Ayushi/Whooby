<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="280dp"
        android:layout_height="87dp"
        android:fontFamily="@font/aclonica"
        android:text="SySagar"
        android:textAlignment="center"
        android:textColor="#C83F3F"
        android:textSize="52sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.158"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sagar"
        app:layout_constraintVertical_bias="0.078" />

    <TextView
        android:id="@+id/preritTV"
        android:layout_width="238dp"
        android:layout_height="84dp"
        android:fontFamily="@font/aclonica"
        android:text="Prerit"
        android:textAlignment="center"
        android:textColor="#C83F3F"
        android:textSize="52sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.183"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/prerit"
        app:layout_constraintVertical_bias="0.355" />

    <TextView
        android:id="@+id/swastikTv"
        android:layout_width="249dp"
        android:layout_height="80dp"
        android:fontFamily="@font/aclonica"
        android:text="Swastik"
        android:textAlignment="center"
        android:textColor="#C83F3F"
        android:textSize="52sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.896"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/swastik"
        app:layout_constraintVertical_bias="0.37" />

    <TextView
        android:id="@+id/ayushiTV"
        android:layout_width="288dp"
        android:layout_height="91dp"
        android:fontFamily="@font/aclonica"
        android:text="Ayushi"
        android:textAlignment="center"
        android:textColor="#C83F3F"
        android:textSize="52sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.919"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ayushi"
        app:layout_constraintVertical_bias="0.082" />

    <ImageView
        android:id="@+id/sagar"
        android:layout_width="419dp"
        android:layout_height="557dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ayushi"
        app:layout_constraintHorizontal_bias="0.243"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022"
        app:srcCompat="@drawable/sysagar" />

    <ImageView
        android:id="@+id/prerit"
        android:layout_width="418dp"
        android:layout_height="502dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/swastik"
        app:layout_constraintHorizontal_bias="0.207"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.828"
        app:srcCompat="@drawable/prerit" />

    <ImageView
        android:id="@+id/swastik"
        android:layout_width="394dp"
        android:layout_height="489dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.941"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.82"
        app:srcCompat="@drawable/swastik_gorai" />

    <ImageView
        android:id="@+id/ayushi"
        android:layout_width="427dp"
        android:layout_height="548dp"
        android:scaleX="1.15"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022"
        app:srcCompat="@drawable/ayushi_kumari" />
</androidx.constraintlayout.widget.ConstraintLayout>